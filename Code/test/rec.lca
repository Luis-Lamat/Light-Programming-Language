program test {

    function fibo(n: int) returns int {
        var a:int
        var b:int
        if (n < 1) do
            return 1
        end
        elsif (n < 2) do
            return 1
        end

        a = fibo(n: n-1)
        b = fibo(n: n-2)
        return (a + b)
        # return fibo(n: n-1) + fibo(n: n-2)
    }
  

    light() {
        print(fibo(n: 5))
    }
}