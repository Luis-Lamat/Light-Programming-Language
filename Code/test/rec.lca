program test {

    function fibo(n: int) returns int {
        var a:int
        if (n < 1) do
            return 1
        end
        elsif (n < 2) do
            return 1
        end
        return fibo(n: n-2) + fibo(n: n-1)
    }
  

    light() {
        window_size(width: 1, height: 1)
        print(fibo(n: 2 ))
    }
}