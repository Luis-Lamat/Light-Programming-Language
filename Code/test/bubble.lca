
program bubble {

    function bubbleSort() {
        var arr:[int](5) = {8, 17, 2, 9, 6}
        var i:int
        var j:int
        var tmp:int

        for (i = 0; i < length(arr); i += 1) do
        	for (j = 0; j < 4 - i; j += 1) do
        		if (arr[j+1] < arr[j]) do
        			tmp = arr[j+1]
        			arr[j+1] = arr[j]
        			arr[j] = tmp
        		end
        	end
        end

    	print(arr)
    }
  

    light() {

        bubbleSort()
    }
}