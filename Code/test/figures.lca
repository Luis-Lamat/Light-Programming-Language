program figures {
	

	light(){

		window_size(width: 500, height: 500)

		figure t2:circle
		
		figure t1:triangle has
			v: (100,200),
			v: (300,400),
			v: (350,250),
			color: rgb((1 + 2 * 8), (4 * 7 + 5), 255)
		end

		t2 has
			v: (100,100),
			color: rgb(100, 200, 200),
			size: 50
		end

		camera t1
		camera t2

		
	}
}