program figures {
	

	light(){

		window_size(width: 100, height: 100)

		figure t2:circle
		

		figure t1:triangle has
			v: (100,200),
			v: (300,400),
			v: (350,250),
			color: rgb((1 + 2 * 8), (4 * 7 + 5), 255)
		end

		t2 has
			v: (1,1),
			color: rgb(1, 2, 2),
			size: 8
		end

		camera t1
	}
}